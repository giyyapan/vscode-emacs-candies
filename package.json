{
    "name": "emacs-candies",
    "displayName": "Emacs Candies",
    "description": "Add some handy emacs-style command which can't be found elsewhere",
    "version": "0.0.1",
    "publisher": "giyyapan",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:emacsCandies.cursorHome",
        "onCommand:emacsCandies.killToEndOfLine"
    ],
    "main": "./out/src/extension",
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
